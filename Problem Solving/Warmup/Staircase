#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main(){
    int n,j,i,k; 
    scanf("%d",&n);
    
    for (i=1;i<(n+1);i++)
        {
         for (j=0;j<(n-i);j++)
         printf(" ");
         for (k=0;k<i;k++)
         printf("#");
        printf("\n");
        
    }
    return 0;
}
