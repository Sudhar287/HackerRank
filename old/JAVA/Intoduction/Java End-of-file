import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner get = new Scanner(System.in);
        int n=1;
        while (get.hasNext()){
            System.out.print(n+" "+get.nextLine()+"\n");
            n++;
        }
    }
}
